# -*- mode: sh -*-

GO(poprc-push-startle) {
  BLOCK({{
    cd ~/src/startle
    git config receive.denyCurrentBranch warn
    cd ~/src/poprc
    git subtree push --prefix=startle ~/src/startle master
  }})
}

POPRC_HOME=~/src/poprc
GO(popr) (
    cd ${POPRC_HOME}
    if [ ! -f eval ]; then
        make eval
    fi
    ./eval -lo *.ppr -im $@
)
